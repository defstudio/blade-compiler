{
    "name": "defstudio/blade-compiler",
    "description": "A standalone laravel blade compiler",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Fabio Ivona",
            "email": "fabio.ivona@defstudio.it",
            "homepage": "https://defstudio.it",
            "role": "Developer"
        },
        {
            "name": "Daniele Romeo",
            "email": "danieleromeo@defstudio.it",
            "homepage": "https://defstudio.it",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0 || ^9.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "DefStudio\\BladeCompiler\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
